<!--전체 데이터 조회를 위한 테이블 form 구현 -->
<!-- employee 데이터 조회를 위한 테이블 form 구현-->
<!-- title 이름은 select.js에서 설정한 그대로-->
<h1>{{ title }}</h1>
<table>
    <!--tr은 행, td는 열을 나타낸다.-->
    <!--attribute -->
    <tr>
        <td>Fname</td> 
        <td>Minit</td> 
        <td>Lname</td> 
        <td>Ssn</td> 
        <td>Bdate</td> 
        <td>Address</td> 
        <td>Sex</td> 
        <td>Salary</td> 
        <td>Super_ssn</td>
        <td>Dno</td> 
    </tr> 
    <!--select.js에서 넘겨준 데이터가 있을 때마다 반복 -->
    {{#each employee}} 
    <tr>
        <td>{{Fname}}</td> 
        <td>{{Minit}}</td> 
        <td>{{Lname}}</td> 
        <td>{{Ssn}}</td> 
        <td>{{Bdate}}</td> 
        <td>{{Address}}</td> 
        <td>{{Sex}}</td> 
        <td>{{Salary}}</td> 
        <td>{{Super_ssn}}</td>
        <td>{{Dno}}</td> 
    </tr>
    {{/each}} 
</table>

<!--department 데이터 조회를 위한 테이블 form 구현-->
<!-- title2 이름은 select.js에서 설정한 그대로-->
<h1>{{ title2 }}</h1>
<table> 
    <tr>
        <td>Dname</td> 
        <td>Dnumber</td> 
        <td>Mgr_ssn</td>
        <td>Mgr_start_date</td> 
    </tr> 
    <!--select.js에서 넘겨준 데이터가 있을 때마다 반복 -->
    {{#each department}} 
    <tr>
        <td>{{Dname}}</td> 
        <td>{{Dnumber}}</td> 
        <td>{{Mgr_ssn}}</td>
        <td>{{Mgr_start_date}}</td> 
    </tr>
    {{/each}} 
</table>

